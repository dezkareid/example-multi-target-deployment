{
  "name": "multi-target-demo",
  "description": "Sample multi target bundling",
  "version": "1.0.0",
  "license": "MIT",
  "scripts": {
    "dev": "webpack-dev-server --mode development --config webpack.dev.config.js",
    "dev:nomodule": "yarn run dev",
    "dev:module": "yarn run dev --env.BROWSERS=module",
    "build": "webpack --mode production --env.BROWSERS=module && webpack --mode production"
  },
  "dependencies": {
    "react": "^16.3.2",
    "react-dom": "^16.3.2"
  },
  "devDependencies": {
    "babel-core": "^6.26.3",
    "babel-loader": "^7.1.4",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "html-webpack-plugin": "^3.2.0",
    "webpack": "^4.8.3",
    "webpack-cli": "^2.1.3",
    "webpack-dev-server": "^3.1.4",
    "webpack-merge": "^4.1.2"
  }
}
